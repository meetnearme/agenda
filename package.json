{
    "name": "local-agenda",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "setup": "node scripts/setup.js",
        "setup:config": "node scripts/setup.js --config",
        "setup:dev": "node scripts/setup-dev.js",
        "setup:remove-template": "node scripts/setup.js --remove-template-only",
        "dev": "next dev --turbopack",
        "dev:cms": "decap-server",
        "dev:all": "pnpm run dev:cms & pnpm run dev",
        "build": "next build",
        "build:ci": "chmod +x scripts/build.sh && ./scripts/build.sh",
        "build:atx-agenda": "SITE_CONFIG=atx-agenda pnpm build:ci",
        "build:marketing": "SITE_CONFIG=marketing pnpm build:ci",
        "build:fork": "pnpm build:ci",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@beehiiv/sdk": "^0.1.9",
        "@hookform/resolvers": "^5.2.1",
        "@radix-ui/react-collapsible": "^1.1.12",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "decap-cms-app": "^3.8.3",
        "gray-matter": "^4.0.3",
        "js-yaml": "^4.1.0",
        "lucide-react": "^0.541.0",
        "next": "16.0.7",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "^7.62.0",
        "rehype-react": "^8.0.0",
        "remark": "^15.0.1",
        "remark-html": "^16.0.1",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@netlify/functions": "^5.1.2",
        "@tailwindcss/postcss": "^4",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "decap-server": "^3.3.1",
        "eslint": "^9",
        "eslint-config-next": "15.4.6",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4",
        "tw-animate-css": "^1.3.7",
        "typescript": "^5"
    }
